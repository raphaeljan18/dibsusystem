<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/">Home</a></li>
    <li class="breadcrumb-item active" aria-current="page">View Document</li>
  </ol>
</nav>

<div class="view-document p-5">
  {{#each rows}}
  <div class="row">
    <div class="col">
      <table class="table" style="width: 100%;">
        <thead>
          <tr>
            <th scope="col">Title</th>
            <th scope="col">Body</th>
            <th scope="col">Assessment</th>
            <th scope="col">Date</th>
            <th scope="col">URL</th>
            <th scope="col">Domestic or Foreign</th>
            <th scope="col">DIB or DISU</th>
            
          </tr>
        </thead>
        <tbody>
          <tr>
            <th scope="row">{{this.title}}</th>
            <td>{{this.body}}</td>
            <td>{{this.assessment}}</td>
            <td>{{this.date}}</td>
            <td>{{this.url}}</td>
            <td>{{this.domfor}}</td>
            <td>{{this.DibOrDisu}}</td>
            <td><a href="/downloadDisuAsWord/{{this.id}}" type="button" class="btn btn-light btn-small"><i class="bi-download"></i> Download {{this.DibOrDisu}} as Word file</a>
</td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
  {{/each}}
</div>